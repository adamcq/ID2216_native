<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_frag"
    app:startDestination="@id/maps">

    <fragment
        android:id="@+id/maps"
        android:name="com.example.map_test1.view.MapsFragment"
        android:label="@string/current_year"
        tools:layout="@layout/fragment_maps" />
    <fragment
        android:id="@+id/info"
        android:name="com.example.map_test1.view.InfoFragment"
        android:label="Color Info"
        tools:layout="@layout/fragment_info" />
    <fragment
        android:id="@+id/year"
        android:name="com.example.map_test1.view.YearFragment"
        android:label="Select Year"
        tools:layout="@layout/fragment_year" >
        <action
            android:id="@+id/action_year_to_maps"
            app:destination="@id/maps" />
    </fragment>
</navigation>